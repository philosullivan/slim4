{
    "name": "nbayramberdiyev/slim-4-starter",
    "type": "project",
    "description": "",
    "authors": [
        {
            "name": "Phil O'Sullivan",
            "email": "phil.osullivan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "php-di/php-di": "^7.0",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.11",
        "slim/twig-view": "^3.3",
        "symfony/translation": "^6.2",
        "symfony/twig-bridge": "^6.2",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
